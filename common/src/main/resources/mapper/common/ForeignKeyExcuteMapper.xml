<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mt.common.system.mapper.ForeignKeyExcuteDao">

    <resultMap id="baseResultMap" type="java.util.HashMap"></resultMap>
    <select id="findAllForeignKeys" parameterType="java.util.HashMap" resultMap="baseResultMap"
            statementType="STATEMENT">
        select eid, name
        from ${tableName}
        where ${fieldName} = ${eid}
    </select>

</mapper>